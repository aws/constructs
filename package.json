{
  "name": "constructs",
  "version": "1.1.2",
  "description": "A programming model for composable configuration",
  "stability": "stable",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "software.constructs",
        "maven": {
          "groupId": "software.constructs",
          "artifactId": "constructs"
        }
      },
      "dotnet": {
        "namespace": "Constructs",
        "packageId": "Constructs"
      },
      "python": {
        "distName": "constructs",
        "module": "constructs"
      }
    }
  },
  "nyc": {
    "reporter": "html",
    "check-coverage": true,
    "lines": 80,
    "statements": 78
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/constructs.git"
  },
  "scripts": {
    "compile": "jsii",
    "watch": "jsii -w",
    "test": "nodeunit test/test.*.js && yarn lint",
    "package": "jsii-pacmak",
    "build": "yarn compile && yarn test && yarn run package",
    "compat": "scripts/compat.sh",
    "lint": "eslint . --ext .ts && yarn compat",
    "bump": "standard-version",
    "release": "jsii-release"
  },
  "keywords": [
    "aws",
    "constructs",
    "cdk",
    "jsii"
  ],
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com",
    "organization": true
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^10.17.17",
    "@types/nodeunit": "^0.0.30",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "jsii": "^0.22.0",
    "jsii-diff": "^0.22.0",
    "jsii-pacmak": "^1.1.0",
    "jsii-release": "^0.1.5",
    "nodeunit": "^0.11.3",
    "nyc": "^15.0.0",
    "standard-version": "^7.1.0"
  },
  "homepage": "https://github.com/aws/constructs",
  "engines": {
    "node": ">= 10.3.0"
  }
}
